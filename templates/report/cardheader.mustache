{{!
    @template report_userattend/report/cardheader

    Quarterly card header.

    Example context (json):
    {
        "quarterid": "12345",
        "quartername": "Quarter 1",
    }
}}

<div class="card-header" role="tab" style="background-color:#f3d9d6;">
    <h5 class="mb-0">
        <a class="collapsed" data-toggle="collapse" href="#quarter-{{quarterid}}" aria-expanded="false" aria-controls="quarter-{{quarterid}}">
            {{quartername}}
            <i class="fa fa-caret-down"></i>
        </a>
    </h5>
</div>
